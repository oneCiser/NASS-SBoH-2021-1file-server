{
  "name": "file-server",
  "version": "1.0.0",
  "description": "servidor de gestion de archivos",
  "main": "src/app.js",
  "scripts": {
    "test": "jest --watchAll --detectOpenHandles",
    "tsc": "tsc --watch",
    "dev": "cross-env NODE_ENV=development nodemon dist/src/app.js",
    "build": "tsc",
    "start": "npm run build && cross-env NODE_ENV=production node dist/src/app.js",
    "pm2": "cross-env NODE_ENV=production pm2 start dist/src/app.js --watch -i max --name file-server",
    "jsdoc": "jsdoc -c jsdoc.json",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --fix --ext .ts"
  },
  "keywords": [],
  "author": "Without author",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "better-docs": "^2.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/archiver": "^5.1.0",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/imagemin-optipng": "^5.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/rsync": "^0.4.30",
    "@types/winston": "^2.4.4",
    "archiver": "^5.3.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "rsync": "^0.6.1",
    "ts-jest": "^26.4.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
